{
  "name": "generator-np",
  "version": "2.2.1",
  "description": "Generator for npm module",
  "keywords": [
    "npm",
    "yeoman",
    "generator",
    "bootstrap"
  ],
  "license": "MIT",
  "repository": "d4rkr00t/generator-np",
  "scripts": {
    "audit": "nsp package",
    "babel-node": "babel-node",
    "build": "babel -d ./ ./src",
    "check": "npm run audit && npm outdated --depth 0",
    "lint": "eslint src test",
    "precheck": "npm run validate",
    "prepublish": "npm run readme | npm run build",
    "readme": "node ./node_modules/node-readme/bin/node-readme.js",
    "test": "mocha test/*.test.js -r babel/register -r test/setup.js",
    "validate": "npm run lint && npm run build && npm test"
  },
  "author": {
    "name": "Stanislav Sysoev",
    "email": "d4rkr00t@gmail.com",
    "url": "http://github.com/d4rkr00t"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.0",
    "eslint": "^1.3.1",
    "mocha": "^2.2.5",
    "node-readme": "^0.1.8",
    "nsp": "^1.1.0",
    "pre-commit": "^1.1.1",
    "yeoman-assert": "^2.0.0"
  },
  "dependencies": {
    "normalize-url": "^1.3.0",
    "underscore.string": "^3.2.0",
    "yeoman-generator": "^0.20.3"
  }
}
