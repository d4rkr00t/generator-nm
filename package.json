{
  "name": "generator-np",
  "version": "2.2.4",
  "description": "Generator for npm module",
  "keywords": [
    "npm",
    "yeoman",
    "generator",
    "bootstrap"
  ],
  "license": "MIT",
  "repository": "d4rkr00t/generator-np",
  "scripts": {
    "audit": "nsp package",
    "build": "babel -d ./ ./src",
    "check": "npm run audit && npm outdated --depth 0",
    "lint": "eslint src test",
    "readme": "node ./node_modules/node-readme/bin/node-readme.js",
    "test": "mocha test/*.test.js -r babel/register -r test/setup.js",
    "validate": "npm run lint && npm test",
    "prepare": "npm run validate && npm run check && npm run readme && npm run build",
    "pp": "npm run prepare"
  },
  "author": {
    "name": "Stanislav Sysoev",
    "email": "d4rkr00t@gmail.com",
    "url": "http://github.com/d4rkr00t"
  },
  "pre-commit": [
    "validate"
  ],
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "cz-conventional-changelog": "^1.1.4",
    "eslint": "^1.7.0",
    "mocha": "^2.3.3",
    "node-readme": "^0.1.9",
    "nsp": "^1.1.0",
    "pre-commit": "^1.1.1",
    "yeoman-assert": "^2.1.0"
  },
  "dependencies": {
    "normalize-url": "^1.3.1",
    "underscore.string": "^3.2.2",
    "yeoman-generator": "^0.20.3"
  },
  "czConfig": {
    "path": "./node_modules/cz-conventional-changelog"
  }
}
