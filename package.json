{
  "name": "generator-np",
  "version": "0.0.0-semantically-released",
  "description": "Generator for npm module with ES6+, Babel, Ava, ESlint, Travis and npm scripts",
  "files": [
    "app/"
  ],
  "keywords": [
    "ava",
    "babel",
    "boilerplate",
    "bootstrap",
    "cli",
    "commitizen",
    "coveralls",
    "es2015",
    "es6",
    "eslint",
    "generator",
    "javascript",
    "kit",
    "module",
    "node_module",
    "node-module",
    "node",
    "npm scripts",
    "npm",
    "plugin",
    "productivity",
    "scaffold",
    "starter",
    "template",
    "transpile",
    "travis",
    "yeoman-generator",
    "yeoman"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/d4rkr00t/generator-np"
  },
  "scripts": {
    "audit": "nsp check package",
    "build": "babel -d ./ ./src",
    "check": "npm run audit && npm outdated --depth 0",
    "coverage": "nyc --reporter=lcov --reporter=text --reporter=html npm test",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "lint": "eslint src test",
    "prepare": "npm run validate && npm run check && npm run build",
    "pp": "npm run prepare",
    "test": "ava --serial",
    "test:watch": "chokidar 'test/index.js' 'src/**' -c 'npm test' --silent --initial",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "validate": "npm run lint && npm test"
  },
  "author": {
    "name": "Stanislav Sysoev",
    "email": "d4rkr00t@gmail.com",
    "url": "http://github.com/d4rkr00t"
  },
  "pre-commit": [
    "validate"
  ],
  "devDependencies": {
    "ava": "^0.7.0",
    "babel": "^6.3.13",
    "babel-eslint": "^4.1.5",
    "chokidar-cli": "^1.2.0",
    "coveralls": "^2.11.4",
    "cz-conventional-changelog": "^1.1.4",
    "eslint": "^1.10.1",
    "nsp": "^2.0.0",
    "nyc": "^4.0.1",
    "pre-commit": "^1.1.1",
    "semantic-release": "^4.3.5",
    "yeoman-assert": "^2.1.0"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "normalize-url": "^1.3.1",
    "underscore.string": "^3.2.2",
    "yeoman-generator": "^0.21.0",
    "yosay": "^1.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
