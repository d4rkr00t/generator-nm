{
  "name": "generator-np",
  "version": "0.0.0-semantically-released",
  "description": "Generator for npm module",
  "keywords": [
    "npm",
    "yeoman",
    "generator",
    "bootstrap"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/d4rkr00t/generator-np"
  },
  "scripts": {
    "audit": "nsp check package",
    "build": "babel -d ./ ./src",
    "check": "npm run audit && npm outdated --depth 0",
    "lint": "eslint src test",
    "prepare": "npm run validate && npm run check && npm run build",
    "pp": "npm run prepare",
    "test": "mocha test/*.test.js -r babel/register",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "validate": "npm run lint && npm test"
  },
  "author": {
    "name": "Stanislav Sysoev",
    "email": "d4rkr00t@gmail.com",
    "url": "http://github.com/d4rkr00t"
  },
  "pre-commit": [
    "validate"
  ],
  "devDependencies": {
    "babel": "^6.0.15",
    "babel-eslint": "^4.1.3",
    "cz-conventional-changelog": "^1.1.4",
    "eslint": "^1.8.0",
    "mocha": "^2.3.3",
    "nsp": "^2.0.0",
    "pre-commit": "^1.1.1",
    "semantic-release": "^4.3.5",
    "yeoman-assert": "^2.1.0"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "normalize-url": "^1.3.1",
    "underscore.string": "^3.2.2",
    "yeoman-generator": "^0.21.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
