{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= moduleDescription %>",<% if (cli) { %>
  "bin": "cli.js",<% } %>
  "main": "lib/index.js",
  "keywords": [
    "npm",
    "yeoman",
    "generator",
    "bootstrap"
  ],
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= moduleName %>",
  "scripts": {
    "audit": "nsp package",
    "babel-node": "babel-node",
    "build": "babel -d ./ ./src",
    "check": "npm run audit && npm outdated --depth 0",
    "cov": "babel-node ./node_modules/.bin/isparta cover --report lcovonly --report html _mocha -- test/*.test.js -r test/setup.js",
    "coveralls": "npm run cov && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "lint": "eslint src test",
    "precheck": "npm run validate",
    "prepublish": "npm run readme | npm run build",
    "postinstall": "npm run prepublish",
    "readme": "node ./node_modules/node-readme/bin/node-readme.js",
    "test": "mocha test/*.test.js -r babel/register -r test/setup.js",
    "validate": "npm run lint && npm run build && npm test"
  },
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= website %>"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {<% if (cli) { %>
    "meow": "^3.3.0",
    "chalk": "^1.1.1"
  <% } %>},
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-eslint": "^4.0.10",
    "chai": "^3.2.0",
    "coveralls": "^2.11.4",
    "eslint": "^1.2.1",
    "isparta": "^3.0.3",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2",
    "node-readme": "^0.1.8",
    "nsp": "^1.1.0",
    "pre-commit": "^1.1.1"
  }
}
